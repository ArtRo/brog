<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:mvc="http://www.springframework.org/schema/mvc"
       xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd
        http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-3.1.xsd"
       default-lazy-init="false" default-autowire="byName">


    <!-- <import resource="applicationContext-db.xml"/> -->
    <context:property-placeholder
            ignore-resource-not-found="false"
            location="classpath*:application.properties,classpath*:dubbo.properties,
				classpath*:redis.properties,classpath*:mongoDb.properties" file-encoding="UTF-8"/>
    <import resource="application-redis.xml"/>
    <import resource="app-dubbo.xml"/>
    <!--<import resource="application-mongodb.xml"/>-->
    <mvc:annotation-driven/>
    <mvc:default-servlet-handler/>
    <!--<bean class="cn.cosfun.config.SwaggerConfig"/>-->
    <!--<bean class="cn.cosfun.config.QiNiuConfig"/>-->

</beans>